<% if (user) {%>
  <div class="likeDislikeDiv">
    <form action="/userlike/<%=user._id%>/<%=user.post[i]._id%>/<%=i%>/?_method=PUT" method="POST">
      <input type="submit" value="Like">
    </form>
    <form action="/userdislike/<%=user._id%>/<%=user.post[i]._id%>/<%=i%>/?_method=PUT" method="POST">
      <input type="submit" value="Dislike">
    </form>
  </div>
<%} else {%>
  <div class="likeDislikeDiv">
    <form action="/like/<%=allUsers[i]._id%>/<%=allUsers[i].post[0]._id%>/0/?_method=PUT" method="POST">
      <input type="submit" value="Like">
    </form>
    <form action="/dislike/<%=allUsers[i]._id%>/<%=allUsers[i].post[0]._id%>/0/?_method=PUT" method="POST">
      <input type="submit" value="Dislike">
    </form>
  </div>
<%}%>
