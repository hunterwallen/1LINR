<div class="likeDislikeDiv">
    <% let likedBy = allUsers[i].post[0].likedBy
        let isLikedBy = false
        for (j in likedBy) {
          if(likedBy[j] === currentUser._id) {
            isLikedBy = true
            break
          }
        }
    if(!isLikedBy) {%>
      <form action="/like/<%=allUsers[i]._id%>/<%=allUsers[i].post[0]._id%>/0/?_method=PUT" method="POST">
        <input type="submit" value="Like">
      </form><span> | </span>
    <%} else {%>
      <form action="/unlike/<%=allUsers[i]._id%>/<%=allUsers[i].post[0]._id%>/0/?_method=PUT" method="POST">
        <input type="submit" value="Remove Like">
      </form><span> | </span>
    <%}
    let dislikedBy = allUsers[i].post[0].dislikedBy
        let isDislikedBy = false
        for (x in dislikedBy) {
          if(dislikedBy[x] === currentUser._id) {
            isDislikedBy = true
            break
          }
        }
    if(!isDislikedBy) {%>
      <form action="/dislike/<%=allUsers[i]._id%>/<%=allUsers[i].post[0]._id%>/0/?_method=PUT" method="POST">
        <input type="submit" value="Dislike">
      </form>
      <%} else {%>
        <form action="/undislike/<%=allUsers[i]._id%>/<%=allUsers[i].post[0]._id%>/0/?_method=PUT" method="POST">
          <input type="submit" value="Remove Dislike">
        </form>
        <%}%>
</div>
